{
  "name": "filter_ui",
  "version": "0.2.3",
  "description": "UI to filter anything",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "yarn run build && electron --inspect ./js/Infrastructure/main_app.js",
    "package_win64": "yarn run build && electron-packager . filter_ui --platform=win32 --arch=x64 --overwrite --out=build",
    "postinstall": "electron-builder install-app-deps",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder",
    "publish-win": "yarn run build && electron-builder build --win --publish onTagOrDraft",
    "test": "yarn build && yarn jest"
  },
  "main": "./js/Infrastructure/main_app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zored/filter_ui"
  },
  "keywords": [
    "filter",
    "ui",
    "electron",
    "typescript"
  ],
  "author": "zored",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "12",
    "@types/rimraf": "^3.0.0",
    "@types/sharp": "^0.25.0",
    "ejs": "^3.1.3",
    "electron": "8.2.3",
    "electron-builder": "^22.6.0",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "spectron": "^10.0.1",
    "ts-jest": "^26.0.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/object-hash": "^1.3.3",
    "electron-updater": "^4.3.1",
    "object-hash": "^2.0.3",
    "sharp": "^0.25.2",
    "yargs": "^15.3.1"
  }
}
